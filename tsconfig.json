{
   "extends": "astro/tsconfigs/strict",
   "include": [".astro/types.d.ts", "**/*"],
   "exclude": ["dist"],
   "compilerOptions": {
      "strict": false,
      "baseUrl": ".",
      "paths": {
         "@components/": ["src/components/"],
         "@layouts/": ["src/layouts/"],
         "@pages/": ["src/pages/"],
         "@styles/": ["src/styles/"]
      },
      "jsx": "react-jsx",
      "jsxImportSource": "react"
   }
}
