{
   "extends": "astro/tsconfigs/strict",
   "include": [".astro/types.d.ts", "**/*"],
   "exclude": ["dist"],
   "compilerOptions": {
      "strict": false,
      "baseUrl": ".",
      "paths": {
         "@/*": ["src/*"],
         "@components/*": ["src/components/*"],
         "@layouts/*": ["src/layouts/*"],
         "@pages/*": ["src/pages/*"],
         "@styles/*": ["src/styles/*"],
         "@assets/*": ["src/assets/*"]
      },
      "jsx": "react-jsx",
      "jsxImportSource": "react"
   }
}
